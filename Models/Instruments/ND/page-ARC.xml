	<layers>

<!-- Center ARC Background -->
		<layer>
			<name>ROSE</name>
			<w>360</w>
			<h>360</h>
			<texture>
				<path>Aircraft/A330-200/Models/Instruments/ND/res/hsiarc.png</path>
