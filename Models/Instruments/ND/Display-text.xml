<background>Aircraft/A320neo/Models/Instruments/ND/res/transparent.png</background>
<w>400</w>
<h>256</h>

<instruments>
  
    <instrument include="page-ARC-text.xml">      
        <condition>
            <and>
            <greater-than>
                <property>systems/electrical/outputs/efis</property>
